            <Button Content="Init Mission" Click="Init_Mission" Margin="11,331,0,0" VerticalAlignment="Top"/>

            <Button Content="Load Mission" Click="Load_Mission" Margin="11,372,0,0" VerticalAlignment="Top"/>
            <TextBlock x:Name="LoadMissionError"  Margin="200,378,1062,585" Text="Load Mission Error" TextWrapping="Wrap"/>

            <Button Content="GetLoaded Mission" Click="Get_Loaded_Mission" Margin="11,412,0,0" VerticalAlignment="Top"/>
            <TextBlock x:Name="GetLoadedMission"  Margin="200,418,1096,541" Text="Get Loaded Mission Error" TextWrapping="Wrap"/>

            <Button Content="Upload Mission" Click="Upload_Mission" Margin="11,452,0,0" VerticalAlignment="Top"/>
            <TextBlock x:Name="UploadMissionError"  Margin="200,458,1092,506" Text="Upload Mission Error" TextWrapping="Wrap"/>

            <Button Content="Execute Mission" Click="Execute_Mission" Margin="11,492,0,0" VerticalAlignment="Top"/>
            <TextBlock x:Name="ExecuteMissionError"  Margin="200,498,1080,477" Text="Execute Mission Error" TextWrapping="Wrap"/>

            <Button Content="Socket Send" Margin="430,580,0,0" Click="Socket_Send" VerticalAlignment="Top"/>
            <TextBlock x:Name="SocketSend" Margin="550,586,-68,0" Text="Send_Data" TextWrapping="Wrap" VerticalAlignment="Top"/>

            <Button Content="Socket Get " Margin="430,620,0,0" Click="Socket_Get" VerticalAlignment="Top"/>
            <TextBlock x:Name="SocketGet" Margin="550,626,-68,0" Text="Get_Data" TextWrapping="Wrap" VerticalAlignment="Top"/>

            <Button Content="WaypointMissionState" Click="Get_WaypointMissionState" Margin="11,674,0,0" VerticalAlignment="Top"/>
            <TextBlock x:Name="WaypointMissionState" Margin="200,680,-68,0" Text="Waypoint Mission State" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock x:Name="UploadState" Margin="200,710,-68,0" Text="Upload State" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock x:Name="DownloadState" Margin="200,740,-68,0" Text="Download State" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <TextBlock x:Name="ExecutionState" Margin="200,770,-68,0" Text="Execution State" TextWrapping="Wrap" VerticalAlignment="Top"/>

            <!--<SwapChainPanel x:Name="swapChainPanel" Visibility="Visible" Margin="10,557,833,10" />-->

        </Grid>

    </Grid>
</Page>

    